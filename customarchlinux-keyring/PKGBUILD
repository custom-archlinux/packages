pkgname=customarchlinux-keyring
pkgver=20210107
pkgrel=1
pkgdesc='Custom Arch Linux PGP keyring'
arch=('any')
url='https://github.com/custom-archlinux'
license=('GPL')
install="${pkgname}.install"
source=('Makefile' 'customarchlinux.gpg')
sha256sums=(
  '510fe998d687792204e2d0c2330e807c5581130deb64b391af2da4d19aa8659d'
  '014e7dc6bd554aa372d1d9c1a532b2bcfff6c058ed8c0a946160717563accc10')

package() {
	cd "${srcdir}"
	make PREFIX=/usr DESTDIR=${pkgdir} install
}
